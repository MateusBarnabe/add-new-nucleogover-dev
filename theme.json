        },
        "elements" : {
          "link" : {
            ":hover" : {
              "typography" : {
                "textDecoration" : "none"
              }
            },
            "color" : {
              "text" : "var(--wp--preset--color--contrast-2)"
            },
            "typography" : {
              "textDecoration" : "none",
              "fontWeight" : "bold"
            }
          }
        },
        "typography" : {
          "fontSize" : "var(--wp--preset--font-size--small)"
        }
