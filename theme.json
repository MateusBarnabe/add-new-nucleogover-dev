        },
        "typography" : {
          "fontSize" : "var(--wp--preset--font-size--small)",
          "fontStyle" : "normal",
          "fontWeight" : "500"
        }
      },
      "caption" : {
        "color" : {
          "text" : "var(--wp--preset--color--contrast-2)"
        },
        "typography" : {
          "fontFamily" : "Roboto, sans-serif",
          "fontSize" : "0.8rem"
        }
      },
      "h1" : {
        "typography" : {
          "fontSize" : "var(--wp--preset--font-size--xx-large)",
          "lineHeight" : "1.15"
        }
      },
      "h2" : {
        "typography" : {
          "fontSize" : "",
          "fontWeight" : "bold"
        }
      },
      "h3" : {
        "typography" : {
          "fontSize" : "var(--wp--preset--font-size--large)"
        }
      },
      "h4" : {
