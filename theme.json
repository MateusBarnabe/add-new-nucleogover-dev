          "fontSize" : "var(--wp--preset--font-size--medium)"
        }
      },
      "h6" : {
        "typography" : {
          "fontSize" : "var(--wp--preset--font-size--small)"
        }
      },
      "heading" : {
        "color" : {
          "text" : "var(--wp--preset--color--contrast)"
        },
        "typography" : {
          "fontFamily" : "Roboto, sans-serif",
          "fontWeight" : "400",
          "lineHeight" : "1.2"
        }
      },
      "link" : {
        "typography" : {
          "textDecoration" : "none"
        },
        "color" : {
          "text" : "var(--wp--preset--color--contrast)"
        }
      }
    },
    "spacing" : {
      "blockGap" : "0",
      "padding" : {
        "left" : "0",
        "right" : "0"
      }
    },
    "typography" : {
      "fontFamily" : "Roboto, sans-serif",
      "fontSize" : "var(--wp--preset--font-size--medium)",
      "fontStyle" : "normal",
      "fontWeight" : "400",
      "lineHeight" : "1.55"
    },
    "css" : ".wp-site-blocks{display:flex;flex-direction:column;min-height:100vh;}.wp-block-group{flex:1;}.wp-query{padding-top:56px}.post-block{position: relative;}.post-block .post-title {position: absolute;top: 50%;left: 50%;transform: translate(-50%, -50%);z-index: 10;color: white;}.post-block a {position: absolute;top: 0;left: 0;width: 100%;height: 100%;z-index: 5;text-indent: -9999px;}.post-block .wp-block-post-terms {position: relative;z-index: 15;}.post-block:hover{color:#0b2a7a;}"
  },
  "templateParts" : [
    {
      "area" : "header",
